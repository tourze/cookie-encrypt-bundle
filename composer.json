{
  "name": "tourze/cookie-encrypt-bundle",
  "description": "A Symfony Bundle for automatically encrypting and decrypting specific cookies",
  "license": "MIT",
  "require": {
    "ext-SPL": "*",
    "symfony/config": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/event-dispatcher": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-foundation": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/yaml": "^7.3",
    "tourze/bundle-dependency": "1.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "tourze/phpunit-base": "1.*",
    "tourze/phpunit-symfony-kernel-test": "0.0.*",
    "tourze/phpunit-symfony-unit-test": "0.0.*",
    "tourze/symfony-dependency-service-loader": "0.0.*",
    "tourze/symfony-testing-framework": "0.0.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\CookieEncryptBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\CookieEncryptBundle\\Tests\\": "tests"
    }
  }
}